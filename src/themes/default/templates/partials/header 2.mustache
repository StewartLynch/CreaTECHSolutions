<header>
    <a id="logo" href="{{site.baseUrl}}">
        <img
            src="{{site.baseUrl}}images/logo.png"
            alt="Logo of {{site.title}}"
            title="{{site.title}}"
        >
    </a>
    <h1>CreaTECH Solutions (Stewart Lynch)</h1>
    <nav>
        <div class="navigation">
            {{#navigation}}
            <a href="{{site.baseUrl}}{{url}}"{{#class}} class="{{.}}"{{/class}}>{{label}}</a>
            {{/navigation}}
        </div>
    </nav>
</header>
